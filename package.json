{
  "name": "nest-next-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && next build && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "env-cmd --silent typeorm-ts-node-commonjs migration:create",
    "migration:run": "env-cmd --silent ts-node -r tsconfig-paths/register ./src/server/script/typeorm_migration_run.ts",
    "migration:rollback": "env-cmd --silent ts-node -r tsconfig-paths/register ./src/server/script/typeorm_migration_rollback.ts",
    "migration:rollbackAll": "env-cmd --silent ts-node -r tsconfig-paths/register ./src/server/script/typeorm_migration_rollback_all.ts",
    "entity:create": "env-cmd --silent typeorm-ts-node-commonjs entity:create",
    "seeder:run": "env-cmd --silent node dist/server/seeder",
    "seeder:refresh": "env-cmd --silent node dist/server/seeder --refresh",
    "seeder:run:dev": "env-cmd --silent ts-node -r tsconfig-paths/register src/server/seeder",
    "seeder:refresh:dev": "env-cmd --silent ts-node -r tsconfig-paths/register src/server/seeder --refresh"
  },
  "dependencies": {
    "@ashrhmn/nest-modules": "^0.0.16",
    "@ashrhmn/z-rest": "^0.0.4",
    "@ashrhmn/z-rest-client": "^0.0.5",
    "@enzoferey/ethers-error-parser": "^0.2.3",
    "@hookform/resolvers": "^3.2.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@tanstack/react-query": "^4.32.6",
    "argon2": "^0.31.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "jsonwebtoken": "^9.0.1",
    "nestjs-seeder": "^0.3.2",
    "next": "^13.4.13",
    "pg": "^8.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@next/eslint-plugin-next": "^13.4.13",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.75",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.9",
    "@types/react": "18.2.19",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
